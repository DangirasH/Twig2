{% extends "layout.html.twig" %}

{% block title %} {{ parent() }} - products{% endblock %}

{% block content %}
    {% for product in products %}
        {% if loop.index0 % 3 == 0 and loop.index0 != 0 %}
            <br>
        {% endif %}
        {{ product }}
    {% endfor %}
    {{ dump(products) }}
{% endblock %}